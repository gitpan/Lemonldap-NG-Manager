CREATE TABLE lmConfig (
		cfgNum int not null primary key,
		locationRules text,
		exportedHeaders text,
		globalStorage text,
		globalStorageOptions text,
		macros text,
		groups text,
		portal text,
		domain text,
		ldapServer text,
		ldapPort int,
		ldapBase text,
		securedCookie int,
		cookieName text,
		authentication text,
		exportedVars text,
		managerDn text,
		managerPassword text,
        timeout int,
		whatToTrace text
		);
